    "test:examples": "npm run build && ./run_examples.sh",
    "watch:bsb": "bsb -make-world -w",
    "watch:jest": "jest --watchAll",
    "watch:screen": "screen -c .screenrc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plow-technologies/bs-aeson.git"
  },
  "keywords": [
    "bucklescript",
    "json",
    "aeson",
    "haskell"
  ],
  "author": "mchaver",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/plow-technologies/bs-aeson/issues"
  },
  "homepage": "https://github.com/plow-technologies/bs-aeson#readme",
  "dependencies": {
    "bs-platform": "^2.1.0"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.3.2"
  }
}
